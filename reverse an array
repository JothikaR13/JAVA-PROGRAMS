import java.Util.Array;
public class example{
  public static void main(Stirng[]args)
  {
    int[]my_array={1,2,3,4};
    System.out.println("Original_array:"+Arrays.toString(my_array));
    for(int i=0;i<my_array.length/2;i++){
      int temp=my_array[i];
      my_array[i]=my_array[my_array.length-i-1];
      my_array[my_array.length-i-1]=temp;
      }
    System.out.println("Reverse_array:"+Arrays.toString(my_array));
    }}

      
